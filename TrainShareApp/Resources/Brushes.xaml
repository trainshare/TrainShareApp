<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<LinearGradientBrush x:Key="ListItemBrush" EndPoint="0.5,1" StartPoint="0.5,0">
		<GradientStop Color="#FFEDEDED" Offset="1"/>
		<GradientStop Color="White"/>
	</LinearGradientBrush>
	<LinearGradientBrush x:Key="MainBackground" StartPoint="0,0" EndPoint="0,1">
		<GradientStop Color="#E52323" Offset="0.0" />
		<GradientStop Color="#A81213" Offset="1.0" />
	</LinearGradientBrush>
	<!-- Resource dictionary entries should be defined here. -->
</ResourceDictionary>